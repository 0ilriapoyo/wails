{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Wails: Debug {{.ProjectName}}",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/{{.PathToDesktopBinary}}",
      "preLaunchTask": "build",
      "cwd": "${workspaceFolder}",
      "env": {},
      "args": [
        "-assetdir",
        "{{.AssetDir}}"
      ]
    }
  ]
}